{
  "name": "doapp",
  "version": "1.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "kill-ports": "tsx server/scripts/killPort.ts",
    "kill-all": "tsx server/scripts/killPort.ts --all",
    "cleanup": "npm run cleanup:force && echo. && echo ============================================ && echo [INICIANDO SERVIDORES AHORA...] && echo ============================================ && echo. && npm run start:servers",
    "cleanup:ps": "npm run cleanup:force && echo. && echo ============================================ && echo [INICIANDO SERVIDORES AHORA...] && echo ============================================ && echo. && npm run start:servers",
    "cleanup:force": "powershell -ExecutionPolicy Bypass -File server/scripts/killPort.ps1",
    "start:servers": "concurrently -n \"CLIENT,SERVER\" -c \"cyan,yellow\" \"npm run dev\" \"npm run dev:server\" --kill-others-on-fail",
    "dev": "vite",
    "dev:server": "powershell -ExecutionPolicy Bypass -File server/scripts/start-dev.ps1",
    "dev:server:direct": "nodemon --watch server --exec tsx server/index.ts",
    "dev:all": "npm run cleanup",
    "build": "vite build",
    "build:client": "vite build",
    "build:server": "echo 'Server runs with tsx - no compilation needed'",
    "preview": "vite preview",
    "start": "tsx server/index.ts",
    "typecheck": "tsc --noEmit",
    "test": "cross-env NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --watch",
    "test:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "test:sql": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config jest.config.sql.js",
    "test:sql:watch": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config jest.config.sql.js --watch",
    "test:sql:coverage": "cross-env NODE_OPTIONS=--experimental-vm-modules jest --config jest.config.sql.js --coverage",
    "test:disputes": "cross-env NODE_OPTIONS=--experimental-vm-modules jest tests/routes/disputes.test.ts",
    "test:admin": "cross-env NODE_OPTIONS=--experimental-vm-modules jest tests/routes/admin-disputes.test.ts",
    "test:email": "cross-env NODE_OPTIONS=--experimental-vm-modules jest tests/services/email.test.ts",
    "migrate:sql": "tsx server/scripts/migrateToPostgreSQL.ts",
    "seed": "tsx server/scripts/seed.ts",
    "seed:ads": "tsx server/scripts/seedAdvertisements.ts",
    "seed:blogs": "tsx server/scripts/seedBlogs.ts",
    "seed:mockup": "tsx server/scripts/seedMockup.ts",
    "test:redis": "tsx server/scripts/testRedis.ts",
    "test:paypal": "tsx server/scripts/testPaypal.ts",
    "test:paypal:escrow": "tsx server/scripts/testPaypalEscrow.ts",
    "test:email:send": "tsx server/scripts/testEmail.ts",
    "debug:payment": "tsx server/scripts/debugPayment.ts",
    "security:check": "tsx server/scripts/securityCheck.ts",
    "security:audit": "npm audit && npm audit fix",
    "lint": "eslint client",
    "lint:hooks": "eslint client",
    "test:all": "npm run lint:hooks && npm run typecheck && npm test"
  },
  "dependencies": {
    "@paypal/checkout-server-sdk": "^1.0.3",
    "@sendgrid/mail": "^8.1.6",
    "@types/express-session": "^1.18.2",
    "@types/nodemailer": "^7.0.2",
    "@types/pdfkit": "^0.17.3",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.2",
    "clsx": "^2.1.1",
    "cookie-parser": "^1.4.7",
    "cors": "^2.8.5",
    "dompurify": "^3.2.7",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "express-rate-limit": "^8.1.0",
    "express-session": "^1.18.2",
    "express-validator": "^7.2.1",
    "firebase": "^12.5.0",
    "firebase-admin": "^13.5.0",
    "helmet": "^8.1.0",
    "i18next": "^25.5.3",
    "i18next-fs-backend": "^2.6.0",
    "i18next-http-middleware": "^3.8.1",
    "isomorphic-dompurify": "^2.28.0",
    "jsonwebtoken": "^9.0.2",
    "lucide-react": "^0.539.0",
    "mailgun.js": "^12.1.0",
    "mercadopago": "^2.10.0",
    "mongoose": "^8.19.2",
    "multer": "^2.0.2",
    "node-cron": "^4.2.1",
    "nodemailer": "^7.0.9",
    "passport": "^0.7.0",
    "passport-facebook": "^3.0.0",
    "passport-google-oauth20": "^2.0.0",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "qrcode": "^1.5.4",
    "rate-limiter-flexible": "^8.0.1",
    "react": "^18.3.1",
    "react-datepicker": "^8.8.0",
    "react-dom": "^18.3.1",
    "react-dropzone": "^14.3.8",
    "react-helmet-async": "^2.0.5",
    "react-i18next": "^16.0.0",
    "react-quill": "^2.0.0",
    "react-router-dom": "^6.30.1",
    "recharts": "^3.3.0",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.7",
    "sequelize-typescript": "^2.1.6",
    "sharp": "^0.34.4",
